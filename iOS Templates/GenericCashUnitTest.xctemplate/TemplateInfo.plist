<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>DefaultCompletionName</key>
		<string>Myfeature</string>
		<key>Description</key>
		<string>This generates a new UnitTest template.</string>
		<key>Kind</key>
		<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
		<key>Options</key>
		<array>
			<dict>
				<key>Description</key>
				<string>UnitTest name to create</string>
				<key>Identifier</key>
				<string>featureName</string>
				<key>Name</key>
				<string>Feature Name:</string>
				<key>NotPersisted</key>
				<true/>
				<key>Required</key>
				<true/>
				<key>Type</key>
				<string>text</string>
				<key>Default</key>
				<string>UnitTestName</string>
			</dict>
			<dict>
				<key>Default</key>
				<string>___VARIABLE_featureName:identifier___</string>
				<key>Identifier</key>
				<string>productName</string>
				<key>Type</key>
				<string>static</string>
			</dict>

			<dict>
				<key>Default</key>
				<string>___VARIABLE_featureName:identifier___Tests</string>
				<key>Description</key>
				<string>Unit test name</string>
				<key>Identifier</key>
				<string>cashUnitTest</string>
				<key>Name</key>
				<string>Unit test Name:</string>
				<key>Required</key>
				<true/>
				<key>Type</key>
				<string>static</string>
			</dict>
		</array>
		<key>Platforms</key>
		<array>
			<string>com.apple.platform.iphoneos</string>
		</array>
		<key>SortOrder</key>
		<string>0</string>
		<key>Summary</key>
		<string>This template generate a Unit Test to Cashout/Cashin (settable name).</string>
	</dict>
</plist>